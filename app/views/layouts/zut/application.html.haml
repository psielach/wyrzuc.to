!!!
%html
  %head
    %meta{ content: 'text/html; charset=UTF-8', 'http-equiv' => 'Content-Type' }/
    %title= A9n.app_name
    = stylesheet_link_tag 'service', media: 'all'
    /[if lt IE 9]
      <script src='https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js'></script>
      <script src='https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js'></script>
    = csrf_meta_tags
  %body.skin-green
    .wrapper
      = render 'layouts/zut/navbar'
      = render 'layouts/zut/sidebar'
      .content-wrapper
        = render 'layouts/service/content_header'
        %section.content
          .row
            .col-md-12
              = render 'layouts/service/flash_message'
              .box.box-danger
                = yield
    = javascript_include_tag 'zut/application', '//maps.googleapis.com/maps/api/js?v=3&callback=initializeMap'
